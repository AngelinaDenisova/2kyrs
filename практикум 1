//5 Написать программу, которая, реализует диалог с пользователем: запрашивает с клавиатуры два вещественных числа, и выводит на экран результат деления первого числа на второе (вещественные числа выводятся с точностью до 3 знаков после запятой)
using System;
namespace ConsoleApp2
{
    class Program
    {
        static void Main(string[] args)
        {
            double a, b, c;
            Console.Write("a=");
            a=double.Parse(Console.ReadLine());
            Console.Write("b=");
            b =double.Parse(Console.ReadLine());
            c=a/b;
            Console.WriteLine(a + "/" + b + "=" + "{0:.###}",  c);
        }
    }
}
